{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"next dev --turbo\" \"tailwindcss -i styles/globals.css -o styles/dist.css -w\"",
        "build": "tailwindcss -m -i styles/globals.css -o styles/dist.css && next build",
        "start": "next start",
        "lint": "next lint",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@types/node": "18.11.15",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "concurrently": "^7.6.0",
        "eslint": "^8.29.0",
        "eslint-config-next": "13.0.6",
        "next": "13.0.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@vitejs/plugin-react": "^3.0.0",
        "autoprefixer": "^10.4.13",
        "jsdom": "^20.0.3",
        "postcss": "^8.4.20",
        "tailwindcss": "^3.2.4",
        "vitest": "^0.25.8"
    }
}
